<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ninjaone.rmm.orders.domain.model.DeviceOrder" table="devices_orders">
        <composite-id name="id" class="com.ninjaone.rmm.orders.domain.model.DeviceOrderId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>

        <property name="deviceId" length="36" access="field"/>

        <property name="customerId" length="36" access="field"/>

        <component name="name" access="field">
            <property name="value" access="field"/>
        </component>
    </class>
</hibernate-mapping>
