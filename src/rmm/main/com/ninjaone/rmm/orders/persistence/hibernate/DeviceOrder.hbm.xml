<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ninjaone.rmm.orders.domain.model.DeviceOrder" table="customer_devices">
        <composite-id name="id" class="com.ninjaone.rmmcustomers.services.domain.model.CustomerServiceId" access="field">
            <key-property column="id" name="value" length="36" access="field" />
        </composite-id>

        <property name="deviceId" length="36" access="field"/>

        <property name="customerId" length="36" access="field"/>

        <property name="systemName" length="36" access="field"/>
    </class>
</hibernate-mapping>
